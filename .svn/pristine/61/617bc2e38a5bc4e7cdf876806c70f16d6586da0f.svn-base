package model;

public class TempoAgendamentoSugestao {
	
	private Long id;
	private String titulo;
	private String descricao;
	private Integer diaDaSemana;
	private Integer hora;
	private Integer minuto;
	private Integer peso;
	
	public TempoAgendamentoSugestao(Long id, String titulo,
			String descricao, Integer diaDaSemana, Integer hora, Integer minuto,
			Integer peso) {
		this.id = id;
		this.titulo = titulo;
		this.descricao = descricao;
		this.diaDaSemana = diaDaSemana;
		this.hora = hora;
		this.minuto = minuto;
		this.peso = peso;
	}
	
	/**
	 * Construtor de inicialização
	 */
	public TempoAgendamentoSugestao() {
		this.id = -1L;
		this.titulo = "";
		this.descricao = "";
		this.diaDaSemana = -1;
		this.hora = -1;
		this.minuto = -1;
		this.peso = 1;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}

	public Integer getDiaDaSemana() {
		return diaDaSemana;
	}

	public void setDiaDaSemana(Integer diaDaSemana) {
		this.diaDaSemana = diaDaSemana;
	}

	public Integer getHora() {
		return hora;
	}

	public void setHora(Integer hora) {
		this.hora = hora;
	}

	public Integer getMinuto() {
		return minuto;
	}

	public void setMinuto(Integer minuto) {
		this.minuto = minuto;
	}

	public Integer getPeso() {
		return peso;
	}

	public void setPeso(Integer peso) {
		this.peso = peso;
	}
	
}
